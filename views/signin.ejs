<link rel="stylesheet" href="/css/GeneralCSS.css">
<link rel="stylesheet" href="/css/signin.css">
<div class="user">
            <header class="user__header">
                <!-- <img src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/3219/logo.svg" alt="" /> -->
                <h1 class="user__title"> Sign-In Page</h1>
            </header>
            <form class="form" action="/users/createsession" method="POST">                
                <div class="form__group">
                    <input type="email" placeholder="Your Email" name="email" autocomplete="off" class="form__input" required/>
                </div>
                
                <div class="form__group">
                    <input type="password" placeholder="Your Password" name="password" autocomplete="off" class="form__input" required/>
                </div>

                <div class="btn-1">
                    <input type="submit" value="SIGN IN" class="input-button">
                </div>
            </form>
</div>